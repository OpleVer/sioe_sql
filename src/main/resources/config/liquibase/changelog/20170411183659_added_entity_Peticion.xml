    <!--
        Added the entity Peticion.
    -->
    <changeSet id="20170411183659-1" author="jhipster">
        <createTable tableName="peticion">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="numero" type="varchar(255)">
                <constraints nullable="false" />
            </column>

            <column name="nombre_solicitante" type="varchar(255)">
                <constraints nullable="false" />
            </column>

            <column name="paterno_solicitante" type="varchar(255)">
                <constraints nullable="false" />
            </column>

            <column name="materno_solicitante" type="varchar(255)">
                <constraints nullable="false" />
            </column>

            <column name="cargo_solicitante" type="varchar(255)">
                <constraints nullable="false" />
            </column>

            <column name="direccion_solicitante" type="varchar(255)">
                <constraints nullable="false" />
            </column>

            <column name="acto_constar" type="varchar(5000)">
                <constraints nullable="false" />
            </column>

            <column name="fecha" type="timestamp">
                <constraints nullable="false" />
            </column>

            <column name="oficio" type="longblob">
                <constraints nullable="false" />
            </column>
            <column name="oficio_content_type" type="varchar(255)">
                <constraints nullable="false" />
            </column>

            <column name="responsable" type="varchar(255)">
                <constraints nullable="false" />
            </column>

            <column name="status_prevencion" type="bit">
                <constraints nullable="true" />
            </column>

            <column name="oficio_prevencion" type="longblob">
                <constraints nullable="true" />
            </column>
            <column name="oficio_prevencion_content_type" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="notificacion_prevencion" type="longblob">
                <constraints nullable="true" />
            </column>
            <column name="notificacion_prevencion_content_type" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="respuesta_prevencion" type="longblob">
                <constraints nullable="true" />
            </column>
            <column name="respuesta_prevencion_content_type" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="tipo_evaluacion" type="integer">
                <constraints nullable="true" />
            </column>

            <column name="acta_procede" type="longblob">
                <constraints nullable="true" />
            </column>
            <column name="acta_procede_content_type" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="acuerdo_procede" type="longblob">
                <constraints nullable="true" />
            </column>
            <column name="acuerdo_procede_content_type" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="notificacion_procede" type="longblob">
                <constraints nullable="true" />
            </column>
            <column name="notificacion_procede_content_type" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="acuerdo_noprocede" type="longblob">
                <constraints nullable="true" />
            </column>
            <column name="acuerdo_noprocede_content_type" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="notificacion_noprocede" type="longblob">
