            </column>

            <column name="acuerdo_presentacion" type="longblob">
                <constraints nullable="true" />
            </column>
            <column name="acuerdo_presentacion_content_type" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="notificacion_presentacion" type="longblob">
                <constraints nullable="true" />
            </column>
            <column name="notificacion_presentacion_content_type" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="status_trabajo" type="bit">
                <constraints nullable="true" />
            </column>

            <column name="origen_id" type="bigint">
                <constraints nullable="false" />
            </column>

            <column name="peticionario_id" type="bigint">
                <constraints nullable="false" />
            </column>

            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here, do not remove-->
        </createTable>
        <dropDefaultValue tableName="peticion" columnName="fecha" columnDataType="datetime"/>
        
    </changeSet>
</databaseChangeLog>
